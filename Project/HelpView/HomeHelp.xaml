<Window x:Class="Project.HomeHelp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project"
        mc:Ignorable="d"
        Title="Pomoć" Height="350" Width="500" Loaded="Window_Loaded"
        Icon="../Icons/question_icon.ico">
    <Grid x:Name="homeHelp">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="9*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="9*"/>
        </Grid.ColumnDefinitions>
        <WebBrowser Cursor="Arrow" x:Name="browser" Grid.Row="1" Grid.ColumnSpan="3"/>
        <Label Background="DarkGray" Grid.Column="0" Grid.ColumnSpan="3"/>
    </Grid>
</Window>
